#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/DevKay/dev/school/reboot/project/agrisense/backend/node_modules/touch/bin/node_modules:/Volumes/DevKay/dev/school/reboot/project/agrisense/backend/node_modules/touch/node_modules:/Volumes/DevKay/dev/school/reboot/project/agrisense/backend/node_modules:/Volumes/DevKay/dev/school/reboot/project/agrisense/node_modules:/Volumes/DevKay/dev/school/reboot/project/node_modules:/Volumes/DevKay/dev/school/reboot/node_modules:/Volumes/DevKay/dev/school/node_modules:/Volumes/DevKay/dev/node_modules:/Volumes/DevKay/node_modules:/Volumes/node_modules:/node_modules:/Volumes/DevKay/dev/school/reboot/project/agrisense/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/DevKay/dev/school/reboot/project/agrisense/backend/node_modules/touch/bin/node_modules:/Volumes/DevKay/dev/school/reboot/project/agrisense/backend/node_modules/touch/node_modules:/Volumes/DevKay/dev/school/reboot/project/agrisense/backend/node_modules:/Volumes/DevKay/dev/school/reboot/project/agrisense/node_modules:/Volumes/DevKay/dev/school/reboot/project/node_modules:/Volumes/DevKay/dev/school/reboot/node_modules:/Volumes/DevKay/dev/school/node_modules:/Volumes/DevKay/dev/node_modules:/Volumes/DevKay/node_modules:/Volumes/node_modules:/node_modules:/Volumes/DevKay/dev/school/reboot/project/agrisense/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
